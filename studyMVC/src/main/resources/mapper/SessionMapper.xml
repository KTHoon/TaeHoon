<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="kr.or.nextit.session.service.impl.SessionMapper">

	<select id="selectMemberItem"
		parameterType="kr.or.nextit.session.service.LoginVo"
		resultType="kr.or.nextit.session.service.LoginVo">



		SELECT
			mem_id,
			mem_pwd,
			mem_name,
			mem_phone,
			mem_email,
			reg_date
		FROM
			tb_members
		Where mem_id=#{memId}
		and mem_pwd=#{memPwd}









	</select>










</mapper>